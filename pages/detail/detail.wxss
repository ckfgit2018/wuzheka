.header {
	position: relative;
    width: 100%;
    height: 100rpx;
    padding-top: 0.1rpx;
    background-color: rgb(46, 187, 169);
    color: #fff;
    box-sizing: border-box;
}
.swiperWrapper{
	width: 100%;
	height: 280rpx;
	box-sizing: border-box;
}
.header .address {
    width: 20%;
    height: 60rpx;
    font-size: 32rpx;
    font-weight: 200;
    line-height: 60rpx;
    text-align: center;
    margin-top: 16rpx;
}

.header .searchInput {
	position: absolute;
	top: 20rpx;
	left: 180rpx;
    width: 60%;
    height: 64rpx;
    border: 2rpx solid #efefef;
    border-radius: 30rpx;
    background-color: #fff;
    box-sizing: border-box;
}

.searchInput input {
	width: 100%;
	height: 100%;
	padding-left: 20rpx;
	box-sizing: border-box;
    border-radius: 32px;
    text-align: left;
    line-height: 60rpx;
    font-size: 28rpx;
    color: #333;
    overflow: hidden;
}
button{
	width: 80rpx;
	height: 80rpx;
	position: absolute;
	top: 11rpx;
	right: 20rpx;
	border: none;
	padding: 0;
	margin: 0;
	border:none;
	box-sizing: border-box;
	background-color: rgb(46, 187, 169);
}
button .so_img{
	width: 60rpx;
	height: 60rpx;
	position: absolute;
	top: 10rpx;
	right: 10rpx;
}
.so_img image{
	width: 100%;
	height: 100%;
	vertical-align: top;
}
.browseWrapper{
	width: 70%;
	height: 64rpx;
	position: absolute;
	top: 386rpx;
	right: 0rpx;
	padding: 4rpx 0;
	box-sizing: border-box;
	display: flex;
}
.browseWrapper .common{
	flex: 1;
	height: 60rpx;
	line-height: 60rpx;
	text-align: center;
}
.switchBar{
	width: 100%;
    height: 100rpx;
    margin-top: 64rpx;
    display: flex;
    background-color: #fff;
    border-bottom: 2rpx #f1f1f1 solid;
    border-top: 2rpx #f1f1f1 solid;
}
.switchBar .switchTitle{
	flex: 0 0 25%;
	width: 25%;
	height: 100rpx;
	line-height: 100rpx;
	text-align: center;
	z-index: 11;
    color: #ed414a;
    border-bottom: 2rpx #ed414a solid;
}
.switchBar .address_list{
	flex: 1;
	height: 100rpx;
	box-sizing: border-box;
	overflow: hidden;
}
.address_list .swiper{
	width: auto;
	height: 100%;
}
.swiper .swiper-item{
	width: 20% !important;
	height: 100%;
	text-align: center;
	line-height: 100rpx;
	font-size: 28rpx;
}
.swiper_wrapper{
	padding-top: 40rpx;
	width: 100%;
	height: auto;
	padding-bottom: 20rpx;
	box-sizing: border-box;
	background-color: #fff;
	border-bottom: 20rpx solid #eff3f7;
}
.swiper_wrapper .item-list{
	width: 20%;
	display: inline-block;
	vertical-align: top;
	height: auto;
}
.item-list .itemImg{
	width: 60%;
	height: 80rpx;
	border-radius: 50%;
	margin: 0 auto;
}
.itemImg image{
	width: 100%;
	height: 100%;
	border-radius: 50%;
	vertical-align: top;
}
.text{
	text-align: center;
	color: #6F6F6F;
	margin-top: 10rpx;
}
.noticeWrapper{
	width: 100%;
	height: auto;
	display: flex;
	background-color: #fff;
}
.noticeWrapper .notice_content{
	flex: 0 0 30%;
	width: 30%;
	height: 100rpx;
}
.notice_content .noticeImg{
	width: 80rpx;
	height: 80rpx;
	margin-top: 10rpx;
	margin-left: 100rpx;
}
.noticeImg image{
	width: 100%;
	height: 100%;
	vertical-align: top;
}
.noticeWrapper .noticeDesc{
	flex: 1;
	height: 80rpx;
	margin-top: 10rpx;
	padding-left: 10rpx;
	border-left: 2rpx solid #eee;
	box-sizing: border-box;
}
.noticeDesc .swiper-wrapper{
	width: 100%;
	height: 100%;
}
.swiper-wrapper .swiper-item{
	width: 100%;
	height: 100%;
}
.swiper-item .text{
	text-align: left;
	margin-top: 0rpx;
	white-space: nowrap;
}